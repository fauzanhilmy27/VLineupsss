<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fillViewport="true"
    tools:context=".MainActivity"
    tools:ignore="SpeakableTextPresentCheck">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".ui.home.HomeFragment">

    <RelativeLayout
        android:id="@+id/tmt1"
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/backround_relative">

        <androidx.cardview.widget.CardView
            android:id="@+id/card2"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:cardCornerRadius="60dp">

            <ImageView
                android:id="@+id/imageview1"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:scaleType="centerCrop"
                android:src="@drawable/fade" />

        </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="98dp"
            android:layout_below="@+id/card2"
            android:layout_marginTop="10dp"
            android:background="@drawable/relative2">

            <TextView
                android:id="@+id/fade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@+id/gambarRole"
                android:text="Fade"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/gambarRole"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/initiator"

                />
        </RelativeLayout>
    </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tmt2"
            android:layout_width="150dp"
            android:layout_height="200dp"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@+id/tmt1"
            android:background="@drawable/backround_relative">

            <androidx.cardview.widget.CardView
                android:id="@+id/card3"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                app:cardCornerRadius="60dp">

                <ImageView
                    android:id="@+id/imageview2"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/brimstone" />

            </androidx.cardview.widget.CardView>

            <RelativeLayout
                android:layout_width="150dp"
                android:layout_height="98dp"
                android:layout_below="@+id/card3"
                android:layout_marginTop="10dp"
                android:background="@drawable/relative2">

                <TextView
                    android:id="@+id/brim"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_toRightOf="@+id/gambarRole1"
                    android:text="Brimstone"
                    android:textColor="@color/white"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/gambarRole1"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="35dp"
                    android:layout_marginTop="10dp"
                    android:src="@drawable/controller"

                    />
            </RelativeLayout>
        </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tmt3"
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/tmt1"
        android:layout_marginTop="20dp"
        android:background="@drawable/backround_relative">

        <androidx.cardview.widget.CardView
            android:id="@+id/card4"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:cardCornerRadius="60dp">

            <ImageView
                android:id="@+id/imageview3"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:scaleType="centerCrop"
                android:src="@drawable/sova1" />

        </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="98dp"
            android:layout_below="@+id/card4"
            android:layout_marginTop="10dp"
            android:background="@drawable/relative2">

            <TextView
                android:id="@+id/sova"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@+id/gambarRole2"
                android:text="sova"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/gambarRole2"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/initiator"

                />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tmt4"
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/tmt2"
        android:layout_toRightOf="@+id/tmt3"
        android:layout_marginTop="20dp"
        android:background="@drawable/backround_relative">

        <androidx.cardview.widget.CardView
            android:id="@+id/card5"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:cardCornerRadius="60dp">

            <ImageView
                android:id="@+id/imageview4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:src="@drawable/viper1" />

        </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="98dp"
            android:layout_below="@+id/card5"
            android:layout_marginTop="10dp"
            android:background="@drawable/relative2">

            <TextView
                android:id="@+id/viper"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@+id/gambarRole3"
                android:text="Viper"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/gambarRole3"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/controller"

                />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tmt5"
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/tmt3"
        android:layout_marginTop="20dp"
        android:background="@drawable/backround_relative">

        <androidx.cardview.widget.CardView
            android:id="@+id/card6"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:cardCornerRadius="60dp">

            <ImageView
                android:id="@+id/imageview5"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:scaleType="centerCrop"
                android:src="@drawable/kayo" />

        </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="98dp"
            android:layout_below="@+id/card6"
            android:layout_marginTop="10dp"
            android:background="@drawable/relative2">

            <TextView
                android:id="@+id/kayo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@+id/gambarRole4"
                android:text="Kayo"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/gambarRole4"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/initiator"

                />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/tmt6"
        android:layout_width="150dp"
        android:layout_height="200dp"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/tmt3"
        android:layout_toRightOf="@+id/tmt5"
        android:layout_marginTop="20dp"
        android:background="@drawable/backround_relative">

        <androidx.cardview.widget.CardView
            android:id="@+id/card7"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:cardCornerRadius="60dp">

            <ImageView
                android:id="@+id/imageview6"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:scaleType="centerCrop"
                android:src="@drawable/killjoy" />

        </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="98dp"
            android:layout_below="@+id/card7"
            android:layout_marginTop="10dp"
            android:background="@drawable/relative2">

            <TextView
                android:id="@+id/killjoy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_toRightOf="@+id/gambarRole5"
                android:text="KillJoy"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/gambarRole5"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/sentinel"

                />
        </RelativeLayout>
    </RelativeLayout>

    </RelativeLayout>
</androidx.core.widget.NestedScrollView>